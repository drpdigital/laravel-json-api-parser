{
    "name": "drpdigital/laravel-json-api-parser",
    "description": "Laravel integration for the jsonapi.org parser",
    "keywords": ["drpdigital", "json", "api", "parser", "json api parser", "laravel", "laravel-json-api-parser"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Normansell",
            "email": "chris.normansell@drpgroup.com"
        }
    ],
    "require": {
        "drpdigital/json-api-parser": "^0.0",
        "illuminate/validation": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/container": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*|~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Drp\\LaravelJsonApiParser\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Drp\\LaravelJsonApiParser\\JsonApiParserServiceProvider"
            ]
        }
    }
}
